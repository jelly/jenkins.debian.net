# jenkins itself, to start the agents
restrict,from="127.0.0.1,::1" ssh-rsa  jenkins@jenkins.debian.net

# remote schedule packages building for reproducible builds testing, only from alioth.debian.org
restrict,command="/srv/jenkins/bin/reproducible_remote_scheduler.py ${SSH_ORIGINAL_COMMAND#* }",from="5.153.231.21" ssh-rsa  reproducible@moszumanska.debian.org
